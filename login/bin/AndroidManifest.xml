<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.login"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.login.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.login.DashboardActivity"
            android:label="@string/title_activity_dashboard" >
        </activity>
        <activity
            android:name="com.example.login.AdditionActivity"
            android:label="@string/title_activity_addition" >
        </activity>
        <activity
            android:name="com.example.login.FindsqurerootActivity"
            android:label="@string/title_activity_findsqureroot" >
        </activity>
        <activity
            android:name="com.example.login.ChangecolourActivity"
            android:label="@string/title_activity_changecolour" >
        </activity>
        <activity
            android:name="com.example.login.SwappingActivity"
            android:label="@string/title_activity_swapping" >
        </activity>
    </application>

</manifest>
